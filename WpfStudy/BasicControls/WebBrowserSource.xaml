<Window x:Class="WpfStudy.BasicControls.WebBrowserSource"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WebBrowserSource"
        Width="300"
        Height="300"
        DataContext="{Binding RelativeSource={RelativeSource Self}}">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition />
        </Grid.RowDefinitions>

        <StackPanel Orientation="Horizontal">
            <Button Click="Button_Click" Content="Baidu" />
            <Button Content="Google" />
            <Button Content="Baidu" />
            <Button Content="Baidu" />
            <Button Content="Baidu" />
        </StackPanel>

        <WebBrowser Grid.Row="1" />
    </Grid>
</Window>
